<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Otterchat</title>
  <link rel="stylesheet" href="/css/chat.css">
</head>
<body>
  <ul class="pages">
    <li class="chat page">
      <div class="chatArea">
        <ul class="messages"></ul>
      </div>
      <input class="inputMessage" placeholder="Type here..."/>
    </li>
    <li class="login page">
      <div class="form">
        <h3 class="title">Is that your nickname?</h3>
        <input class="usernameInput" type="text" maxlength="14" id='nickname'/>
      </div>
    </li>
  </ul>

  <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
  <script src="/socket.io/socket.io.js"></script>
  <script src="/js/chat.js"></script>
</body>
</html>
