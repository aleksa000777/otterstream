<!DOCTYPE html>
<html>
  <head>
    <% include ./partials/head %>
    <title>Otterstream Profile</title>
  </head>
  <body>

    <% include ./partials/header %>

    <div class="main">
      <div class="navbar">
        <div class="toggleprofilelist">Show All Profiles</div>
        <div class="toggleprojectlist">Show All Projects</div>
        <div class="openprofile">Show Your Profile</div>
      </div>
      <div class="profile">
        <div class="profileform">
          <% include ./partials/profileForm %>
        </div>
        <div class="profilehead">
          <div class="projectform">
            <% include ./partials/projects %>
            <div class="closeprojectform">Close</div>
          </div>
        </div>
        <div class="profilebody"></div>
        <div class="profilefooter">
          <div class="profileprojects row align-center">
            <div class="openprojectform row large-12 align-center">Add Project</div>
          </div>
          <div class="profileblog row align-center large-4">
            <% include ./partials/blog %>
          </div>
        </div>
      </div>
      <div class="profilelist">
        <div class="globalblog"></div>
      </div>
      <div class="project"></div>
      <div class="projectlist"></div>
      <div class="globalblogposts"></div>
      <div class="profileModal">
        <div class="profileModalBody"></div>
        <div class="profileModalFooter">
          <div class="profileProjectsModal"></div>
          <div class="profileBlogModal"></div>
        </div>
      </div>
    </div>
  </body>
</html>
